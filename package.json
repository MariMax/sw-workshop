{
  "name": "gum",
  "version": "1.0.13",
  "description": "",
  "main": "scripts.js",
  "scripts": {
    "clear": "rm -rf ./dist",
    "copy-precaching": "cp ./node_modules/sw-precaching/build/sw-precaching.js ./dist/sw-precaching.js",
    "copy-routing": "cp ./node_modules/sw-routing/build/sw-routing.js ./dist/sw-routing.js",
    "copy-runtime-caching": "cp ./node_modules/sw-runtime-caching/build/sw-runtime-caching.js ./dist/sw-runtime-caching.js",
    "copy-update": "cp ./node_modules/sw-broadcast-cache-update/build/sw-broadcast-cache-update.js ./dist/sw-broadcast-cache-update.js",
    "copy-expiration": "cp ./node_modules/sw-cache-expiration/build/sw-cache-expiration.js ./dist/sw-cache-expiration.js",
    "copy": "npm run copy-precaching && npm run copy-routing && npm run copy-runtime-caching && npm run copy-update && npm run copy-expiration",
    "build-app": "$(npm bin)/webpack --config ./webpack.main.config.js",
    "build-sw": "$(npm bin)/webpack --config ./webpack.sw.config.js",
    "bump-version": "npm version patch",
    "prepush": "npm run bump-version",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js && $(npm bin)/webpack --config ./config/webpack.sw.config.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "dependencies": {
    "idb-keyval": "^2.3.0",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.1.1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-react-app": "^2.2.0",
    "babel-runtime": "^6.20.0",
    "browser-sync": "^2.12.5",
    "case-sensitive-paths-webpack-plugin": "^2.0.0",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-spawn": "^5.1.0",
    "css-loader": "^0.28.1",
    "detect-port": "^1.1.2",
    "dotenv": "^4.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "fs-extra": "^3.0.1",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.4",
    "husky": "^0.13.3",
    "react-dev-utils": "^0.5.2",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack": "2.5.1",
    "webpack-dev-server": "2.4.5",
    "workbox-precaching": "^1.0.0",
    "workbox-routing": "^1.0.0"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
